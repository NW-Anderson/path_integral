          RowBox[{"2", "*", "y"}]}]}], "]"}], "*", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "1"}], "/", "2"}], "*", "m", "*", 
         RowBox[{"(", 
          RowBox[{"m", "+", "1"}], ")"}], "*", "t"}], "]"}]}], ",", 
