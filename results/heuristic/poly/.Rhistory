B2 <- cumsum(rnorm(n/2,0,sqrt(dt)))
t2 <- t1
C2 = (1-t2/last(t2))*0.2 + t2*0.7 / last(t2) + (B2 - t2*last(B2) / last(t2))
C <- c(C1,C2)
