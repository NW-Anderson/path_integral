p1 <- sum(log(dnorm(C1[-1],
mean = C1[-length(C1)],
sd = sqrt(C1[-length(C1)]*(1-C1[-length(C1)])* dt))*dt))
p2 <- sum(log(dnorm(C2[-1],
mean = C2[-length(C2)],
sd = sqrt(C2[-length(C2)]*(1-C2[-length(C2)])* dt))*dt))
